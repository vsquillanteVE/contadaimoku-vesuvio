# Disabilita gli avvisi TypeScript durante la build
TSC_COMPILE_ON_ERROR=true
# Tratta gli avvisi come avvisi, non come errori
ESLINT_NO_DEV_ERRORS=true
# Ignora gli avvisi TypeScript durante la build
VITE_SKIP_TS_CHECK=true
# Forza Vite a ignorare gli errori TypeScript
VITE_TSCONFIG_PATHS=false
# Disabilita completamente il controllo dei tipi
VITE_TS_TRANSPILE_ONLY=true
# Imposta l'ambiente di produzione
NODE_ENV=production
VITE_NODE_ENV=production
